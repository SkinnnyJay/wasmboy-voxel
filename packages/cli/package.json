{
  "name": "@wasmboy/cli",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "bin": {
    "wasmboy-voxel": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "npm run typecheck && npm run format:check",
    "format:check": "npx prettier --check \"src/**/*.ts\" \"test/**/*.ts\" \"README.md\"",
    "test": "vitest run"
  },
  "dependencies": {
    "@wasmboy/api": "file:../api"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.0"
  }
}
