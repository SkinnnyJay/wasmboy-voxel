    "contract:ci:full": "run-s ci:local changeset:status:ci contract:sample:registers",
    "contract:ci:full:strict": "run-s ci:local:strict changeset:status:ci contract:sample:registers",
    "stack:test": "pnpm -r --if-present --filter \"./packages/**\" --filter \"./apps/**\" test",
