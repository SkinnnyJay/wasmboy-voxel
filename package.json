    "test:integration:headless:clean:ci": "run-s test:integration:headless:ci clean:artifacts:precommit",
    "test:integration:headless:callback": "node --experimental-worker node_modules/mocha/bin/_mocha test/integration/headless-callback-test.cjs --timeout 20000 --exit",
