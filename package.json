    "backlog:generate:next100": "node scripts/next-backlog-generator.mjs",
    "workflow:lint": "npx prettier --check \".github/workflows/*.yml\"",
    "workflow:format": "npx prettier --write \".github/workflows/*.yml\"",
    "scripts:lint": "npx prettier --check \"scripts/**/*.mjs\"",
    "scripts:format": "npx prettier --write \"scripts/**/*.mjs\"",
    "workflow:check": "run-s workflow:lint scripts:lint",
    "memory:layout:check": "node scripts/core-wrapper-memory-layout-check.mjs",
    "core:memory-offset:check": "node scripts/core-memory-offset-contract-check.mjs",
    "lint:library:console": "node scripts/check-library-console-usage.mjs",
