    "stack:test:smoke:scope:cli": "pnpm -r --if-present --filter \"./packages/cli\" test -- --help",
    "stack:test:smoke:scope:debugger": "pnpm -r --if-present --filter \"./apps/debugger\" test -- --help",
    "stack:test:smoke:scopes": "run-s stack:test:smoke:scope:api stack:test:smoke:scope:cli stack:test:smoke:scope:debugger",
    "stack:typecheck": "pnpm -r --if-present --filter \"./packages/**\" --filter \"./apps/**\" typecheck",
    "stack:lint": "pnpm -r --if-present --filter \"./packages/**\" --filter \"./apps/**\" lint",
    "changeset": "changeset",
    "changeset:status": "changeset status",
    "changeset:status:ci": "node scripts/changeset-status-ci.mjs",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "security:scan:workspaces": "node scripts/security-scan-workspaces.mjs",
