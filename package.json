  "license": "GPL-3.0-or-later",
  "type": "module",
  "homepage": "https://wasmboy.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SkinnnyJay/wasmboy-voxel.git"
  },
  "bugs": {
    "url": "https://github.com/SkinnnyJay/wasmboy-voxel/issues"
  },
