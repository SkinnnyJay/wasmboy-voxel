    "test:core": "run-s build test:core:savestate test:core:isolation test:core:executeloop test:core:timeroverflow test:core:timer:savestate test:core:timer:batching test:core:interrupts test:core:interrupts:halt test:core:bootrom:init test:core:serial test:core:audio:channel3:dmg test:core:audio:channel3:nr52 test:core:audio:channel4:bounded test:core:audio:vin test:core:memory:banking test:core:sound:bounds test:core:serialization test:core:memorytraps test:core:graphics test:core:graphics:palette test:core:graphics:debugmap test:core:graphics:tilecache",
